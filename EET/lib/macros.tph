	/////////////////////////////////////////////////////////////////////////////////////////////////////
	ELSE /*PATCH_IF ("%filetype%" STR_EQ "Ignoring 0 byte file")*/ BEGIN
		//PATCH_PRINT ~Ignoring %SOURCE_FILESPEC%...~

		SET ignore_check += 1
		SET checked = 1
	END

