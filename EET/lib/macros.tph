		SET off = offset SPRINT res ~eff~ LPM EET_PCU_off_res
	END

	PATCH_IF ("%fx_type%" = 186) BEGIN //Move creature
		SET off = offset SPRINT res ~are~ LPM EET_PCU_off_res
