	OR ("%fx_type%" = 172) //Remove spell
	OR ("%fx_type%" = 200) //Spell turning
	OR ("%fx_type%" = 201) //Spell turning
	OR ("%fx_type%" = 206) //Protection from spell
	OR ("%fx_type%" = 207) //Reflect specified spell
