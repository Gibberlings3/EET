	/////////////////////////////////////////////////////////////////////////////////////////////////////
	//SPL patching                                                                                     //
	/////////////////////////////////////////////////////////////////////////////////////////////////////
	PATCH_IF ("%filetype%" STRING_EQUAL_CASE "SPL V1  ") AND (SOURCE_SIZE > 0x71) BEGIN
		PATCH_PRINT ~%cntcurrent%/%cntrow% Patching spell version 1.0 file %patch_name%...~

		//0x10 casting sound WAV resref
		SET off = 0x10 SPRINT res ~wav~ LPM EET_PCU_off_res

		//0x3a spellbook icon BAM resref
		SET off = 0x3a SPRINT res ~bam~ LPM EET_PCU_off_res

		///////////
		//Abilities
		///////////
		READ_LONG 0x64 "abil_off"
		READ_SHORT 0x68 "abil_cnt"
		FOR (cnt=0; cnt<"%abil_cnt%"; cnt=cnt+1) BEGIN
			//0x4 memorised icon BAM resref
			SET off = ("%abil_off%"+0x28*cnt+0x4) SPRINT res ~bam~ LPM EET_PCU_off_res
			//0x26 Projectile animation (projectl.ids/missile.ids)
			SET off = ("%abil_off%"+0x28*cnt+0x26) LPM EET_PCU_off_ipro
		END

		/////////
		//Effects
		/////////
		READ_LONG 0x6a "fx_off"
		READ_SHORT 0x70 "fx_cnt"

		FOR (cnt=0; cnt<"%abil_cnt%"; cnt=cnt+1) BEGIN
			READ_SHORT ("%abil_off%"+0x28*cnt+0x1e) "fx_num"
			SET fx_cnt += fx_num //gauge the total number of effects by summing global effects and the effects of each ability
		END

		FOR (cnt=0; cnt<"%fx_cnt%"; cnt=cnt+1) BEGIN
			READ_SHORT ("%fx_off%"+0x30*cnt) "fx_type"
			SET offset = ("%fx_off%"+0x30*cnt+0x14)

			PATCH_IF ("%fx_type%" = 55) //Death: Kill Creature Type
			OR ("%fx_type%" = 72) //IDS: Set IDS State
			OR ("%fx_type%" = 100) //Protection: from Creature Type
			OR ("%fx_type%" = 109) //State: Hold
			OR ("%fx_type%" = 175) //State: Hold
			OR ("%fx_type%" = 177) //Use EFF File
			OR ("%fx_type%" = 178) //Spell Effect: THAC0 vs. Creature Type Modifier
			OR ("%fx_type%" = 179) //Spell Effect: Damage vs. Creature Type Modifier
			OR ("%fx_type%" = 185) //State: Hold
			OR ("%fx_type%" = 219) //Stat: AC vs. Creature Type Modifier
			OR ("%fx_type%" = 238) //Death: Disintegrate
			OR ("%fx_type%" = 283) //Use EFF File (Cursed)
			OR ("%fx_type%" = 319) //Usability: Item Usability
			BEGIN
				READ_LONG ("%fx_off%"+0x30*cnt+0x8) "ids_file"
				PATCH_IF (ids_file = 4) BEGIN //Race.ids
					SET off = ("%fx_off%"+0x30*cnt+0x4) SPRINT res ~race~ LPM EET_PCU_off_long
				END ELSE PATCH_IF (ids_file = 5) BEGIN //Class.ids
					SET off = ("%fx_off%"+0x30*cnt+0x4) SPRINT res ~class~ LPM EET_PCU_off_long
				END ELSE PATCH_IF (ids_file = 6) BEGIN //Specific.ids
					SET off = ("%fx_off%"+0x30*cnt+0x4) SPRINT res ~spec~ LPM EET_PCU_off_long
				END
			END

			PATCH_IF ("%fx_type%" = 318) //Protection: Immunity to Spell
			OR ("%fx_type%" = 324) //Protection: Immunity to Spell and Message
			OR ("%fx_type%" = 326) //Add Effects List
			BEGIN
				READ_LONG ("%fx_off%"+0x30*cnt+0x8) "ids_file"
				PATCH_IF (ids_file = 104) BEGIN //Race.ids
					SET off = ("%fx_off%"+0x30*cnt+0x4) SPRINT res ~race~ LPM EET_PCU_off_long
				END ELSE PATCH_IF (ids_file = 105) BEGIN //Class.ids
					SET off = ("%fx_off%"+0x30*cnt+0x4) SPRINT res ~class~ LPM EET_PCU_off_long
				END ELSE PATCH_IF (ids_file = 106) BEGIN //Specific.ids
					SET off = ("%fx_off%"+0x30*cnt+0x4) SPRINT res ~spec~ LPM EET_PCU_off_long
				END
			END

			PATCH_IF ("%fx_type%" = 331) BEGIN //Random Monster Summoning
				SET off = ("%fx_off%"+0x30*cnt+0x8) SPRINT res ~sum~ LPM EET_PCU_off_long
			END

			PATCH_IF ("%fx_type%" = 142) //Display Special Effect Icon
			OR ("%fx_type%" = 169) //Prevent Special Effect Icon
			OR ("%fx_type%" = 240) //Remove Special Effect Icon
			BEGIN
				SET off = ("%fx_off%"+0x30*cnt+0x8) SPRINT res ~state~ LPM EET_PCU_off_long
			END

			PATCH_IF ("%fx_type%" = 214) //Select Spell
			OR ("%fx_type%" = 127) //Monster Summoning
			OR ("%fx_type%" = 331) //Random Monster Summoning
			BEGIN
				SET off = offset SPRINT res ~2da~ LPM EET_PCU_off_res
			END

			PATCH_IF ("%fx_type%" = 186) BEGIN //MoveToArea
				SET off = offset SPRINT res ~are~ LPM EET_PCU_off_res
			END

			PATCH_IF ("%fx_type%" = 68) //Unsummon creature
			OR ("%fx_type%" = 215) //Play 3D Effect
			OR ("%fx_type%" = 296) //Protection from Specific Animation
			OR ("%fx_type%" = 336) //Seven eyes overlay
			BEGIN
				SET off = offset SPRINT res ~bam~ LPM EET_PCU_off_res
			END

			PATCH_IF ("%fx_type%" = 82) BEGIN //Set AI Script
				SET off = offset SPRINT res ~bcs~ LPM EET_PCU_off_res
			END

			PATCH_IF ("%fx_type%" = 107) BEGIN //Change Portrait
				SET off = offset SPRINT res ~bmp~ LPM EET_PCU_off_res
			END

			PATCH_IF ("%fx_type%" = 67) //Summon Creature
			OR ("%fx_type%" = 135) //Polymorph into Specific
			OR ("%fx_type%" = 151) //Replace Creature
			OR ("%fx_type%" = 168) //Remove Creature
			BEGIN
				SET off = offset SPRINT res ~cre~ LPM EET_PCU_off_res
			END

			PATCH_IF ("%fx_type%" = 319) BEGIN //Item Usability
				READ_LONG ("%fx_off%"+0x30*cnt+0x8) "ids_target"
				PATCH_IF (ids_target = 11) BEGIN //Actor's script name
					SET off = offset SPRINT res ~dv~ LPM EET_PCU_off_res
				END
			END

			PATCH_IF ("%fx_type%" = 177) //Use EFF File
			OR ("%fx_type%" = 272) //Apply Repeating EFF
			OR ("%fx_type%" = 283) //Use EFF File (Cursed)
			OR ("%fx_type%" = 248) //Set Melee Effect
			OR ("%fx_type%" = 249) //Set Ranged Effect
			BEGIN
				SET off = offset SPRINT res ~eff~ LPM EET_PCU_off_res
			END

			PATCH_IF ("%fx_type%" = 111) //Create Item in Magical Weapon Slot
			OR ("%fx_type%" = 112) //Remove Item
			OR ("%fx_type%" = 122) //Create Inventory Item
			OR ("%fx_type%" = 123) //Remove Inventory Item
			OR ("%fx_type%" = 143) //Create Item in Slot
			OR ("%fx_type%" = 180) //Cannot Use Item
			OR ("%fx_type%" = 255) //Create Inventory Item (days)
			BEGIN
				SET off = offset SPRINT res ~itm~ LPM EET_PCU_off_res
			END

			PATCH_IF ("%fx_type%" = 152) BEGIN //Play Movie
				SET off = offset SPRINT res ~mve~ LPM EET_PCU_off_res
			END

			PATCH_IF ("%fx_type%" = 146) //Cast Spell (at Creature)
			OR ("%fx_type%" = 147) //Learn Spell
			OR ("%fx_type%" = 148) //Cast Spell (at Point)
			OR ("%fx_type%" = 171) //Give Innate Ability
			OR ("%fx_type%" = 172) //Remove Spell
			OR ("%fx_type%" = 183) //Apply Effect Itemtype (SPL ResRef?)
			OR ("%fx_type%" = 206) //Protection from Spell
			OR ("%fx_type%" = 207) //Bounce Specified Spell
			OR ("%fx_type%" = 232) //Cast Spell on Condition
			OR ("%fx_type%" = 251) //Change Bard Song Effect
			OR ("%fx_type%" = 252) //Set Trap
			OR ("%fx_type%" = 256) //Spell sequencer
			OR ("%fx_type%" = 258) //Activate spell sequencer
			OR ("%fx_type%" = 266) //Remove Protection from Spell
			OR ("%fx_type%" = 313) //High-Level Ability Denotation
			OR ("%fx_type%" = 318) //Immunity to Spell
			OR ("%fx_type%" = 321) //Effects specified by Resource
			OR ("%fx_type%" = 324) //Immunity to spell and message
			OR ("%fx_type%" = 326) //Apply effects list
			OR ("%fx_type%" = 333) //Static Charge
			OR ("%fx_type%" = 335) //Seven Eyes
			OR ("%fx_type%" = 340) //Change Backstab Effect
			OR ("%fx_type%" = 341) //Change Critical Hit Effect
			BEGIN
				SET off = offset SPRINT res ~spl~ LPM EET_PCU_off_res
			END

			PATCH_IF ("%fx_type%" = 187) //Store Local Variable
			OR ("%fx_type%" = 265) //Modify Global Variable
			OR ("%fx_type%" = 309) //Modify Local Variable
			BEGIN
				SET off = offset SPRINT res ~var~ LPM EET_PCU_off_res
			END

			PATCH_IF ("%fx_type%" = 174) BEGIN //Play Sound
				SET off = offset SPRINT res ~wav~ LPM EET_PCU_off_res
			END
		END

		SET spl_check += 1
		SET checked = 1
	END

