		OUTER_SET wavsize = BASH_FOR_SIZE + 8
	END
	COPY ~EET/base/dummy.wavc~ ~EET/base/dummy.wavc~ //Read the constant in wavc header
		READ_ASCII 0x10 wavcheader (12)
	BUT_ONLY
	COPY + ~%output_dir%/%output_file%.acm~ ~%output_dir%/%output_file%~
