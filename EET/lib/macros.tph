//vars: array, src_dir_1, dest_dir, del_pvrz, SOURCE_EXT, reserve_index_start, reserve_index_cnt
DEFINE_PATCH_MACRO ~EET_install_pvrz~ BEGIN
	READ_LONG 0x10 "blocks_cnt"
	PATCH_IF (~%SOURCE_EXT%~ STR_EQ ~bam~) BEGIN
		READ_LONG 0x1c "blocks_off"
	END ELSE BEGIN //mos
		READ_LONG 0x14 "blocks_off"
	END
	FOR (cnt=0; cnt<"%blocks_cnt%"; cnt=cnt+1) BEGIN
		READ_LONG ("%blocks_off%"+0x1c*cnt+0x0) "original_base_index"
		//PATCH_PRINT ~cnt = %cnt%, original_base_index = %original_base_index%~
		PATCH_IF (VARIABLE_IS_SET $EVAL ~%array%~(~%original_base_index%~)) BEGIN
			TEXT_SPRINT new_base_index $EVAL ~%array%~(~%original_base_index%~)
			WRITE_LONG ("%blocks_off%"+0x1c*cnt+0x0) "new_base_index"
		END ELSE BEGIN
			SET new_base_index = reserve_index_start + reserve_index_cnt //variables already set via prep_PVRZ.tph
			SET reserve_index_cnt = reserve_index_cnt + 1
			//PATCH_PRINT ~%array% = %original_base_index% => %new_base_index%~
			SPRINT $EVAL "%array%"("%original_base_index%") "%new_base_index%"
			WRITE_LONG ("%blocks_off%"+0x1c*cnt+0x0) "new_base_index"
			PATCH_FOR_EACH index IN original_base_index new_base_index BEGIN
				SET value = EVAL ~%index%~
				PATCH_IF (value < 10) BEGIN
					SPRINT EVAL ~%index%~ ~000%value%~
				END ELSE PATCH_IF (value < 100) BEGIN
					SPRINT EVAL ~%index%~ ~00%value%~
				END ELSE PATCH_IF (value < 1000) BEGIN
					SPRINT EVAL ~%index%~ ~0%value%~
				END ELSE BEGIN
					SPRINT EVAL ~%index%~ ~%value%~
				END
			END
			INNER_ACTION BEGIN
				//LAF ~INSTALL_PVRZ~ INT_VAR original_base_index new_base_index STR_VAR source_file = EVAL ~%src_dir_1%/mos%pvrz_name%.pvrz~ target_folder = EVAL ~%dest_dir%~ END
				COPY_LARGE + ~%src_dir_1%/mos%original_base_index%.pvrz~ ~%dest_dir%/mos%new_base_index%.pvrz~
				ACTION_IF del_pvrz = 1 BEGIN
					DELETE + ~%src_dir_1%/mos%original_base_index%.pvrz~
				END
			END
		END
	END
END

