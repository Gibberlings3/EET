		//PATCH_IF ver = 2 BEGIN
		//	READ_ASCII offset "str1" (8) NULL
		//	READ_ASCII offset2 "str2" (8) NULL
		//	READ_ASCII offset3 "str3" (8) NULL
		//	SPRINT str123 ~%str1%%str2%%str3%~
		//	SPRINT match ~%str123%~
		//END
