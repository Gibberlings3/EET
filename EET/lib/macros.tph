			PATCH_IF ((StrRef >= min) AND (StrRef <= max)) BEGIN
				SAY offset ( AT "StrRef" )
				SPRINT log ~%log%  SAY %offset% @%StrRef% %slash%%slash%opcode=%fx_type%%LNL%~
