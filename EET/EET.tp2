                        ~ARRIVEA.MOS~ ~BPENDA.MOS~ ~DLCDN_.MOS~ ~DLCNE_.MOS~ ~DLCP1.MOS~ ~DLCV1.MOS~ ~GUICHP0A.MOS~ ~GUICHP0B.MOS~
                        ~GUICHP1A.MOS~ ~GUICHP1B.MOS~ ~GUICHP2A.MOS~ ~GUICHP2B.MOS~ ~GUICHP3A.MOS~ ~GUICHP3B.MOS~ ~GUICHP4A.MOS~ ~GUICHP4B.MOS~
                        ~GUICHP5A.MOS~ ~GUICHP5B.MOS~ ~GUICHP6A.MOS~ ~GUICHP6B.MOS~ ~GUICHP7A.MOS~ ~GUICHP8A.MOS~ ~GUIDRM2.MOS~ ~GUIDRM3.MOS~
                        ~GUIDRM4.MOS~ ~GUIDRM5.MOS~ ~GUIDRM6.MOS~ ~GUIDRM7.MOS~ ~GUIVERB2.MOS~ ~LEAVEA.MOS~ ~TOSCENDA.MOS~ ~ULGOTHST.MOS~
                        ~sod/CH01AJON.MOS~ ~sod/CH02AGLN.MOS~ ~sod/CH03AMON.MOS~ ~sod/CH04ASHP.MOS~ ~sod/CH04BGTH.MOS~ ~sod/CH04CSAH.MOS~ ~sod/CH05AUND.MOS~
                        ~sod/CH06ADRW.MOS~ ~sod/CH07ASUL.MOS~ ~sod/CHPTAMKE.MOS~ ~sod/CHPTMANA.MOS~ ~sod/CHPTODRE.MOS~ ~sod/CHPTSARA.MOS~ BEGIN
	ACTION_IF (~%file%~ STRING_MATCHES_REGEXP ~sod/.+~ = 0) BEGIN
		OUTER_TEXT_SPRINT gui_src_dir ~%MOD_FOLDER%/base/gui/sod~
		OUTER_TEXT_SPRINT gui_dest_dir ~%MOD_FOLDER%/SoD_GUI~
		OUTER_PATCH_SAVE file ~%file%~ BEGIN REPLACE_TEXTUALLY ~^sod/\(.+\)~ ~\1~ END
	END ELSE BEGIN
		OUTER_TEXT_SPRINT gui_src_dir ~%MOD_FOLDER%/base/gui~
		OUTER_TEXT_SPRINT gui_dest_dir ~%biff_dir%~
	END
	COPY + ~%gui_src_dir%/%file%~ ~%gui_dest_dir%~
		READ_LONG 0x10 "blocks_cnt"
		PATCH_IF (~%SOURCE_EXT%~ STR_EQ ~bam~) BEGIN
